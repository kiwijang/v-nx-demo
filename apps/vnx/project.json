{
  "name": "vnx",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/vnx/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/vnx"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nrwl/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "vnx:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "vnx:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "vnx:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nrwl/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "vnx:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "vnx:build:development"
        },
        "production": {
          "buildTarget": "vnx:build:production"
        }
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["coverage/apps/vnx"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/vnx"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/vnx/**/*.ts"]
      }
    }
  }
}
